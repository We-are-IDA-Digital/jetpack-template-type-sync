<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured">
    <head jcr:primaryType="nt:unstructured">
        <clientlibs
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
            categories="[granite.conf.ui.browser,jetpack.configuration-browser.admin.synctemplatetypes]"/>
    </head>
    <actions jcr:primaryType="nt:unstructured">
        <selection jcr:primaryType="nt:unstructured">
            <sync
                granite:rel="jetpack-configuration-browser-admin-actions-sync"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                action="jetpack.configuration-browser.sync"
                activeSelectionCount="single"
                icon="refresh"
                ignoreRel="true"
                text="Sync Template-Types with Templates"
                variant="actionBar"/>
        </selection>
    </actions>
</jcr:root>
